{
  "name": "db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "npm run build:dev",
    "start:prod": "npm run build:prod",
    "build:dev": "set NODE_ENV=development&& nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build:prod": "set NODE_ENV=production&& nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "main": "index.ts",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.12.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
